{
  "name": "Browser Search",
  "description": "Searches through current webpage to find relevant text to user's query",
  "manifest_version": 3,
  "version": "1.0",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "notifications",
    "activeTab",
    "scripting",
    "storage",
    "declarativeContent"
  ],
  "background": {
    "service_worker": "background.js"
  }
}