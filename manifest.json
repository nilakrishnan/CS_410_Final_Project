{
    "manifest_version": 3,
    "name": "Intellegent Browsing",
    "description": "Parse any Chrome web pages for most relevant text to users query.",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      }
    },
    "permissions": ["notifications", "activeTab", "scripting", "storage", "declarativeContent"],
    "background": {
      "service_worker": "background.js"
    }
  }